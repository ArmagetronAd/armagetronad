<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
  	<key>CFBundleDevelopmentRegion</key>
  	  <string>English</string>
  	  
  	<key>CFBundleExecutable</key>
  	  <string>@progtitle@</string>
  	  
  	<key>CFBundleIconFile</key>
  	  <string>desktop/os-x/armagetronad.icns</string>
  	  
  	<key>CFBundleIdentifier</key>
  	  <string>org.armagetronad.@progname@</string>

	<key>CFBundleGetInfoString</key>
	  <string>@progtitle@ @version@, © The Armagetron Advanced Developer Team, 2003, 2013</string>

	<key>NSHumanReadableCopyright</key>
	  <string>© The Armagetron Advanced Developer Team, 2003, 2013</string>
	
	<key>CFBundleShortVersionString</key>
	  <string>@version@</string>

  	<key>CFBundleInfoDictionaryVersion</key>
  	  <string>6.0</string>
  	  
  	<key>CFBundleName</key>
  	  <string>@progtitle@</string>
  	  
  	<key>CFBundlePackageType</key>
  	  <string>APPL</string>
  	  
  	<key>CFBundleSignature</key>
  	  <string>Arma</string>
  	  
	<key>CFBundleURLTypes</key>
  	<array>
  		<dict>
  			<key>CFBundleURLName</key>
  			<string>Armagetron Advanced URL</string>
  			<key>CFBundleURLSchemes</key>
  			<array>
  				<string>armagetronad</string>
  			</array>
  		</dict>
  	</array>
  	
  	<key>LSMinimumSystemVersion</key>
    <string>@os_x_minimum_system_version@</string>
  </dict>
</plist>
